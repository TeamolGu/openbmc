&i2c3 {
	status = "okay";

	eeprom: eeprom@53 {
		compatible = "atmel,24c512";
		pagesize = <128>;
		reg = <0x53>;
		wp-gpios = <&gpio ASPEED_GPIO(A, 0) GPIO_ACTIVE_LOW>;
		num-addresses = <16>;
		lable = "fru";
	};
};

