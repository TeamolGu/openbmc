&i2c6 {
	status = "okay";

	adm1278@10 {
		compatible = "adi,adm1278";
		reg = <0x10>;
		Rsense = <500>;
	};

	tla2024@49 {
		compatible = "ti,tla2024";
		reg = <0x49>;
		#address-cells = <1>;
		#size-cells = <0>;

		channel0@0 {
			reg = <0>;
			ti,gain = <2>;
			ti,datarate = <4>;
		};

		channel1@1 {
			reg = <1>;
			ti,gain = <2>;
			ti,datarate = <4>;
		};

		channel2@0 {
			reg = <2>;
			ti,gain = <2>;
			ti,datarate = <4>;
		};

		channel3@1 {
			reg = <3>;
			ti,gain = <2>;
			ti,datarate = <4>;
		};
	};

	ina220@40 {
		compatible = "ti,ina220";
		reg = <0x40>;
		shunt-resistor = <1000>;
	};

	ina220@44 {
		compatible = "ti,ina220";
		reg = <0x44>;
		shunt-resistor = <1000>;
	};

/* 	ina220@42 {
